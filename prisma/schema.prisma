generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Category {
  id   String @id @default(uuid())
  name String
}

model Companion {
  id          String @id @default(uuid())
  userId      String
  useName     String
  src         String
  name        String @db.Text
  description String
  instruction String @db.Text
  seed        String @db.Text

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
